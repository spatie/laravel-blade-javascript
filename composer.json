{
    "name": "spatie/laravel-blade-javascript",
    "description": "A Blade directive to export variables to JavaScript",
    "keywords": [
        "spatie",
        "laravel",
        "blade",
        "javascript",
        "directive",
        "laravel-blade-javascript"
    ],
    "homepage": "https://github.com/spatie/laravel-blade-javascript",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        },
        {
            "name": "Sebastian De Deyne",
            "email": "sebastian@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^8.0|^9.0|^10.0",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "pestphp/pest": "^1.22"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\BladeJavaScript\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\BladeJavaScript\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\BladeJavaScript\\BladeJavaScriptServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
